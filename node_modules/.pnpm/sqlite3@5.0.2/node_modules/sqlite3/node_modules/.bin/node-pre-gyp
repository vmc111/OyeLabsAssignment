#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/sqlite3@5.0.2/node_modules/sqlite3/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/sqlite3@5.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/sqlite3@5.0.2/node_modules/sqlite3/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS/node_modules/.pnpm/sqlite3@5.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../../../../node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
